{
    "manifest_version": 2,
    "name": "Rowella",
    "version": "1",
    "minimum_chrome_version": "27",
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "app": {
        "background": {
            "scripts": ["app.js"]
        }
    },
    "permissions": [
        "https://docs.google.com/feeds/",
        "https://docs.googleusercontent.com/",
        "experimental",
        "identity"
    ]
}
